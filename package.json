{
  "name": "wfm",
  "version": "1.0.0",
  "scripts": {
    "analyze": "webpack-bundle-analyzer dist/stats-es2015.json",
    "ng": "ng",
    "start": "npm run start:local-dev-api",
    "start:production": "ng serve --c=production",
    "start:dev-azure": "ng serve --c=dev-azure",
    "start:stage-azure": "ng serve --c=stage-azure",
    "start:local-dev-api": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --c=local-dev-api",
    "start:prod-azure": "ng serve --c=prod-azure",
    "start:none": "ng serve",
    "build": "ng build",
    "build:dev-azure": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build -c dev-azure",
    "build:stage-azure": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build -c stage-azure",
    "build:prod-azure": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build -c prod-azure",
    "build:stats": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --c prod-azure --stats-json && npm run analyze",
    "test": "ng test",
    "lint": "ng lint",
    "prettier": "prettier --config \"./.prettierrc\" --write \"**/*.{ts,scss,html}\""
  },
